            <component
              refId="Package\Data Flow Task\OLE DB Source"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    /* spare request main info */
    sr.sucode              AS spare_request_code,
    sr.suno                AS spare_request_number,
    sr.s_sudate            AS spare_request_date_shamsi,
    sr.m_sudate            AS spare_request_date_gregorian,
    sr.accountcode         AS account_code,
    sr.accountname         AS account_name,
    sr.statename           AS state_name,
    sr.cityname            AS city_name,
    sr.telno               AS phone_number,
    sr.address             AS address,
    sr.regname             AS request_registered_by,
    sr.confirmname         AS request_confirmed_by,
    sr.archive             AS is_archived,
    sr.closed              AS is_closed,

    /* spare request items */
    sru.sqcode             AS request_use_code,
    sru.spcode             AS part_code,
    sru.spname             AS part_name,
    sru.partno             AS part_number,
    sru.requestno          AS requested_quantity,
    sru.confirmno          AS confirmed_quantity,
    sru.finalno            AS final_quantity,
    sru.cancelno           AS canceled_quantity,
    sru.backorderno        AS backorder_quantity,
    sru.unitprice          AS unit_price,
    sru.discountpercent    AS discount_percent,
    sru.discount           AS discount_amount,
    sru.totalprice         AS total_price,
    sru.requestkind        AS request_type,
    sru.whcode             AS request_warehouse_code,

    /* warehouse inventory uses */
    u.uscode               AS inventory_use_code,
    u.gcode                AS inventory_group_code,
    u.gname                AS inventory_group_name,
    u.section              AS inventory_section,
    u.inputoutput          AS inventory_input_output,
    u.invoiceno            AS inventory_invoice_number,
    u.invoicecode          AS inventory_invoice_code,
    u.no                   AS inventory_quantity,
    u.buyprice             AS inventory_buy_price,
    u.saleprice            AS inventory_sale_price,
    u.avgbuyprice          AS inventory_average_buy_price,

    /* owning invoice uses */
    ou.uncode              AS owning_use_code,
    ou.invoiceno           AS owning_invoice_number,
    ou.invoicecode         AS owning_invoice_code,
    ou.s_regdate           AS owning_register_date,
    ou.no                  AS owning_quantity,
    ou.saleprice           AS owning_sale_price,
    ou.buyprice            AS owning_buy_price,
    ou.totalprice          AS owning_total_price,

    /* warehouse transfer (havaleh) */
    wh.wvcode              AS transfer_code,
    wh.havalehno           AS transfer_number,
    wh.s_regdate           AS transfer_register_date,
    wh.fromwhcode          AS transfer_from_warehouse_code,
    wh.towhcode            AS transfer_to_warehouse_code,
    wh.havalehkind         AS transfer_type,
    wh.totalprice          AS transfer_total_price,
    wh.regname             AS transfer_registered_by,
    wh.exited              AS transfer_is_exited,
    wh.recieved            AS transfer_is_received,
    wh.closed              AS transfer_is_closed,

    /* transfer items */
    hu.hucode              AS transfer_item_code,
    hu.no                  AS transfer_item_quantity,
    hu.basesaleprice       AS transfer_item_base_sale_price,
    hu.saleprice           AS transfer_item_sale_price,
    hu.totalprice          AS transfer_item_total_price,
    hu.received            AS transfer_item_is_received,

    /* warehouse info */
    w.whname               AS warehouse_name,
    w.warehousecode        AS warehouse_full_code,
    w.whserialwork         AS warehouse_serial_control,

    /* part master data */
    wr.spname              AS master_part_name,
    wr.partno              AS master_part_number,
    wr.intcode             AS internal_part_code,
    wr.partbrand           AS part_brand,
    wr.partapp             AS part_application,
    wr.partmodel           AS part_model,
    wr.gcode               AS master_group_code,
    wr.gname               AS master_group_name,
    wr.balance             AS current_stock_balance,
    wr.owningbalance       AS owning_stock_balance,
    wr.avgbuy              AS average_buy_price,
    wr.lastbuy             AS last_buy_price,
    wr.salep               AS sale_price,
    wr.gsalep              AS general_sale_price,
    wr.min                 AS minimum_stock_level,
    wr.location            AS warehouse_location,
    wr.serialwork          AS is_serial_controlled

FROM dbo.sparerequest sr

LEFT JOIN dbo.sparerequestuses sru
    ON sr.sucode = sru.sucode

LEFT JOIN dbo.uses u
    ON sru.spcode = u.spcode
   AND sru.partno = u.partno

LEFT JOIN dbo.owninguses ou
    ON sru.spcode = ou.spcode
   AND u.invoicecode = ou.invoicecode
   AND u.invoiceno   = ou.invoiceno

LEFT JOIN dbo.havalehuses hu
    ON sru.spcode = hu.spcode
   AND ou.invoicecode = hu.usescode

LEFT JOIN dbo.whhavaleh wh
    ON hu.wvcode = wh.wvcode

LEFT JOIN dbo.warehouses w
